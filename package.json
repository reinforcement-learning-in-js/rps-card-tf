{
  "name": "rps-card-tf",
  "version": "0.1.0",
  "description": "rock-paper-scissors card game AI module with tensorflow js",
  "main": "dist/index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "webpack --config ./webpack/source.prod.js",
    "compile": "webpack --config ./webpack/source.dev.js",
    "www-compile": "webpack --config ./webpack/www.dev.js",
    "www": "webpack --config ./webpack/www.prod.js",
    "start": "webpack serve --config ./webpack/www.dev.js",
    "run": "http-server ./www/build -p 3000"
  },
  "keywords": [
    "reinforcement-learning",
    "counterfactual-regret-minimization",
    "webgame",
    "rock-scissors-paper",
    "ai"
  ],
  "author": "fifteenmania",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.6",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "mocha": "^9.2.1",
    "node-typescript": "^0.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
